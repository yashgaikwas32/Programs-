#palindrome
#include <iostream>
#include <stack>
#include <cctype>
#include <string>

using namespace std;

bool isPalindrome(string str) {
    stack<char> s;
    
    // Push alphanumeric characters to the stack in lowercase
    for (char ch : str) {
        if (isalnum(ch)) {
            s.push(tolower(ch));
        }
    }
    
    // Check if the string is a palindrome
    for (char ch : str) {
        if (isalnum(ch)) {
            if (s.top() != tolower(ch)) {
                return false;
            }
            s.pop();
        }
    }
    
    return true;
}

int main() {
    string str;
    cout << "Enter a string: ";
    getline(cin, str);

    if (isPalindrome(str)) {
        cout << "The string is a palindrome." << endl;
    } else {
        cout << "The string is not a palindrome." << endl;
    }

    return 0;
}
